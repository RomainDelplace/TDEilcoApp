<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_popular">

    <fragment
        android:id="@+id/navigation_popular"
        android:name="com.example.tp_film.ui.home.FramePopular"
        android:label="Popular"
        tools:layout="@layout/frame_popular" />

    <fragment
        android:id="@+id/navigation_favorite"
        android:name="com.example.tp_film.ui.home.FrameFavorite"
        android:label="Favorite"
        tools:layout="@layout/frame_favorite" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.tp_film.ui.home.FrameSearch"
        android:label="Search"
        tools:layout="@layout/frame_search" />

    <fragment
        android:id="@+id/navigation_upcoming"
        android:name="com.example.tp_film.ui.home.FrameUpcoming"
        android:label="Upcoming"
        tools:layout="@layout/frame_upcoming" />
</navigation>